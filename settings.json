{
    "files.exclude": {
        "**/*.pyc": true,
        "**/__pycache__": true,
        "**/*.egg-info": true,
        "**/.pytest_cache": true,
        "*.coverage": true,
        "*.coverage.*": true,
        "requirements*.txt": true,
        "**/.tox": true,
        ".venv": true,
        "build": true,
        "dist": true,
        "htmlcov": true,
        "docs/build": true,
        "node_modules": true,
        "cdk.out": true,
        "**/build-*": true,
        "**pid**": true,
        "**/*.eggs": true,
        "**/.cache": true,
        "**/*.ipynb_checkpoints": true
    },
    "terminal.integrated.scrollback": 10000,
    "editor.rulers": [78],
    "editor.fontFamily": "Fira Code, Menlo, Monaco, Consolas, Ubuntu Mono",
    "editor.tabSize": 4,
    "editor.renderWhitespace": "all"
}